{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ES6",
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react",
    "moduleResolution": "Node",
    "resolveJsonModule": true
  },
  "include": ["./src/**/*"]
}